<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:ci="sap.ciconnect.control" xmlns:layout="sap.ui.layout" >
	<layout:Splitter width="100%" resize="onContainerResize">
		<layout:contentAreas>
			<TileContainer
				id="pipelineList"
				tiles="{pipeline>/}">
				<StandardTile
					title="{pipeline>name}" />
			</TileContainer>
		</layout:contentAreas>
	</layout:Splitter>
</core:FragmentDefinition>