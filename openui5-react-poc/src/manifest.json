{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "sap.ciconnect",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-FST"
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_belize_plus"]
  	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "sap.ciconnect.view.App",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": { }
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ciconnect.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ciconnect.view",
				"controlId": "ciconnectApp",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
		               "target": "notFound"
		          }
			},
			"routes": [{
				"pattern": "",
				"name": "appHome",
				"target": "home"
			}, {
				"pattern": "pipeline/{index}",
				"name": "pipeline",
				"target": "pipeline"
			}, {
				"pattern": "repository/{index}",
				"name": "repository",
				"target": "repository"
			}],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"pipeline": {
					"viewName": "Pipeline",
					"viewLevel": 2
				},
				"repository": {
					"viewName": "Repository",
					"viewLevel": 2
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "fade",
					"viewLevel": 10
				}
			}
		}
	}
}