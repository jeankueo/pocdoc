<mvc:View
	controllerName="sap.ciconnect.view.summary"
	xmlns="sap.suite.ui.microchart"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m">
		<m:GenericTile press="press" header="This is a TwoByOne tile with" subheader="two RadialMicroChart" frameType="TwoByOne">
			<m:tileContent>
				<m:TileContent footer="Some footer">
					<m:content>
						<RadialMicroChart percentage="100" valueColor="Good"/>
					</m:content>
				</m:TileContent>
				<m:TileContent footer="Some footer">
					<m:content>
						<RadialMicroChart percentage="0" valueColor="Error"/>
					</m:content>
				</m:TileContent>
			</m:tileContent>
		</m:GenericTile>
		<m:GenericTile press="press" header="This is OneByOne tile with" subheader="a RadialMicroChart" frameType="OneByOne">
			<m:tileContent>
				<m:TileContent footer="Some footer">
					<m:content>
						<RadialMicroChart percentage="99.9" valueColor="Neutral"/>
					</m:content>
				</m:TileContent>
			</m:tileContent>
		</m:GenericTile>
</mvc:View>